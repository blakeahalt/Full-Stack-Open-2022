{
  "name": "part5",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.27.2",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "jsonwebtoken": "^9.0.0",
    "mongoose-unique-validator": "^3.1.0",
    "process": "^0.11.10",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "cross-env react-scripts start",
    "dev": "concurrently \"cd .. && cd part4 && cross-env node index.js\" \"npm start\"",
    "dev:test": "concurrently \"cd .. && cd part4 && cross-env NODE_ENV=test node index.js\" \"npm start\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "json-server -p3001 --watch db.json",
    "build:ui": "rm -rf build && cd ../part2-notes/ && npm run build && cp -r build ../notes-backend",
    "deploy": "git push heroku main",
    "deploy:full": "npm run build:ui && git add . && git commit -m uibuild && git push && npm run deploy",
    "logs:prod": "heroku logs --tail",
    "eslint": "eslint .",
    "cypress:open": "cypress open",
    "cy:open": "start-server-and-test start http://work.local:3030 cy:run"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:cypress/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "cypress": "^12.4.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^27.2.1",
    "json-server": "^0.17.0",
    "jsonwebtoken": "^9.0.0",
    "react-error-overlay": "6.0.9",
    "start-server-and-test": "^1.15.3"
  },
  "resolutions": {
    "mini-css-extract-plugin": "2.4.5"
  }
}
